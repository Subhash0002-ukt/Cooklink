    <script>
        function showRecipe(title, ingredients, steps, nutrition) {
    const recipeDetails = document.getElementById("recipe");

    // Check if the recipe is already visible
    if (recipeDetails.style.display === "block") {
        // If it's visible, hide it again
        recipeDetails.style.display = "none";
    } else {
        // Otherwise, show the recipe details
        document.getElementById("recipe-title").innerText = title;

        // Setting the content of each box with the respective recipe data
        document.getElementById("ingredients-list").innerText = ingredients.join("\n");
        document.getElementById("steps-list").innerText = steps.join("\n");
        document.getElementById("nutrition-info").innerText = nutrition;
        
        recipeDetails.style.display = "block";
        
        // Scroll the page automatically to the recipe section
        window.scrollTo({
            top: recipeDetails.offsetTop - 20,  // Adjust to avoid navbar overlap
            behavior: 'smooth'
        });
    }
}
// Auto-scroll function
// Auto-scroll function


// Trigger the auto-scroll function when the page loads
window.onload = function() {
    autoScroll();
};



    </script>
